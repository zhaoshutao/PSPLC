    <Action Name="A_PowerOff" Id="{f8455d76-f208-4ad8-b331-bf9b4a5057da}">
      <Implementation>
        <ST><![CDATA[OffBusy:=TRUE;
CASE OffState OF
	0:
		IF NOT PhaseShifterLin_MC_Power.Status THEN	(*¶¼Ã»ÉÏµç*)
			OffState:=88;
		ELSE
			OffState:=1;
		END_IF
	1:
		PhaseShifterLin_MC_Power.Enable:=FALSE;
		A_PowerMC();
		OffState:=2;
	2:
		IF NOT PhaseShifterLin_MC_Power.Status THEN	(*¶¼Ã»ÉÏµç*)
			OffState:=88;
		ELSE
			ErrorId:=11;
			OffState:=-100;
		END_IF
	88:
		Off:=FALSE;
		OffState:=0;
		OffBusy:=FALSE;
		OnStatus:=FALSE;
		OffStatus:=TRUE;
		Error:=FALSE;
		ErrorID:=0;
	-100:
		FB_PlcEmergencyStop();
		Off:=FALSE;
		OffState:=-100;
		OffBusy:=FALSE;
		OffStatus:=FALSE;
		Error:=TRUE;

END_CASE]]></ST>
      </Implementation>
    </Action>
    <Action Name="A_PowerOn" Id="{46ba4774-a9a2-4a51-9400-f8277a7731df}">
      <Implementation>
        <ST><![CDATA[OnBusy:=TRUE;
fb_ton(PT:=t#0.5s);
CASE OnState OF
	0:	(*Check*)
		A_PowerMC();
		IF PhaseShifterLin_MC_Power.Error OR NOT PhaseShifterLin_MC_Power.Active THEN		(*FB Error*)
			ErrorID:=1;
			OnState:=99;
		ELSIF PhaseShifterLin_MC_Power.Status THEN		(*ÖáÒÑ¾­¶¼PowerOn£¬²»·ûºÏ¹¦ÄÜ¿éµ÷ÓÃÌõ¼þ*)
			ErrorID:=2;
			OnState:=-100;
		ELSIF NOT PhaseShifterLin_MC_Power.Status THEN		(*Öá¶¼Ã»ÓÐPower£¬×ªÏÂÒ»²½*)
			OnState:=1;
		ELSE
			ErrorID:=3;
			OnState:=-100;
		END_IF
	1:	(*Execute*)
		IF NOT PhaseShifterLin_MC_Power.Status THEN		(*ÔÙ¼ì²é£¬Öá¶¼Ã»ÓÐPower*)
			PhaseShifterLin_MC_Power.Enable:=TRUE;
			A_PowerMC();
			fb_ton.IN:=TRUE;
			OnState:=2;
		ELSE
			ErrorID:=4;
			OnState:=-100;
		END_IF
	2:	(*Wait*)
		A_PowerMC();
		IF NOT PhaseShifterLin_MC_Power.Status THEN		(*Power StatusÐÅºÅ²»µ½£¬¼ÌÐøµÈ´ý£¬Èç¹û³¬Ê±ÔòFB Error*)
			IF NOT fb_ton.Q THEN
				OnState:=2;
			ELSE
				ErrorID:=8;
				OnState:=99;
			END_IF
		ELSIF  PhaseShifterLin_MC_Power.Status THEN			(*ÕýÈ·µç»úÉÏµã×´Ì¬£¬×ªÏÂÒ»²½*)
				OnState:=88;
		ELSE
				ErrorID:=9;
				OnState:=-100;
		END_IF
	88:
		On:=FALSE;
		OnState:=0;
		OnBusy:=FALSE;
		OnStatus:=TRUE;
		OffStatus:=FALSE;
		Error:=FALSE;
		ErrorID:=0;
		fb_ton.IN:=FALSE;
	99:
		On:=FALSE;
		OnState:=99;
		OnBusy:=FALSE;
		OnStatus:=FALSE;
		Error:=TRUE;
		PhaseShifterLin_MC_Power.Enable:=FALSE;
		fb_ton.IN:=FALSE;
	-100:
		FB_PlcEmergencyStop();
		On:=FALSE;
		OnState:=-100;
		OnBusy:=FALSE;
		OnStatus:=FALSE;
		Error:=TRUE;
		PhaseShifterLin_MC_Power.Enable:=FALSE;
		fb_ton.IN:=FALSE;

END_CASE]]></ST>
      </Implementation>
