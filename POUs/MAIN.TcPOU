	NOT KS_BottomFar_Contact1 OR
	NOT KS_BottomFar_Contact2 OR
	NOT KS_BottomNear_Contact1 OR
	NOT KS_BottomNear_Contact2 OR
	NOT KS_TopFar_Contact1 OR
	NOT KS_TopFar_Contact2 OR
	
