    <Action Name="A_LEHeartBeatMonitor" Id="{c15e81d3-3944-4229-9ecc-53c2921ba7f3}" FolderPath="Check\">
      <Implementation>
        <ST><![CDATA[//monitor only while power on and not simulation or not use LE
IF NOT st_Power THEN
	RETURN;
END_IF
(**)
IF simulation THEN
	RETURN;
END_IF
IF NOT USELinearEncoder THEN
	RETURN;
END_IF

// 初始化
IF LEHeartBeatFirstScan THEN
    Velo_PhaseShifterLin_Last := PhaseShifterLin.NcToPlc.ActVelo;
    LEHeartBeatFirstScan := FALSE;
END_IF;

// 计算光栅前后速度差，速度差比位置差更敏感
DeltaVelo_PhaseShifterLin := ABS(PhaseShifterLin.NcToPlc.ActVelo - Velo_PhaseShifterLin_Last);
//PhaseShifter monitor
IF DeltaVelo_PhaseShifterLin > LEHeartBeatDeadZone THEN
    // 有变动，光栅正常，计数器归零，更新位置,重置所有状态
    StableTimer_PhaseShifterLin(IN := FALSE, PT := LEHeartBeatAlarmTime);
    Alarm_PhaseShifterLin := FALSE;
    Velo_PhaseShifterLin_Last := PhaseShifterLin.NcToPlc.ActVelo;  // 
    StableTime_PhaseShifterLin := T#0S;
	IsInitialPositionSet_PhaseShifter:=FALSE;
	Alarm_PhaseShifterMotorPos:=FALSE;
	Alarm_PhaseShifterMotorVelo:=FALSE;
ELSE
    //一点变动没有了，光栅异常，开始计时
    StableTimer_PhaseShifterLin(
        IN := TRUE,
        PT := LEHeartBeatAlarmTime,
        ET => StableTime_PhaseShifterLin
    );
	//记录电机初始位置，后后续对比做准备
	IF NOT IsInitialPositionSet_PhaseShifter THEN
        Pos_PhaseShifterMotor_Initial := Pos_PhaseShifterMotor;  // 记录初始位置
        IsInitialPositionSet_PhaseShifter := TRUE;
   	END_IF;
    
    // 100ms速度完全不变，开始监控电机速度和位置
    IF StableTimer_PhaseShifterLin.Q THEN
        Alarm_PhaseShifterLin := TRUE;  // 
			//电机位置对比及监测
			MotorPosChange_PhaseShifter:=ABS( Pos_PhaseShifterMotor-Pos_PhaseShifterMotor_Initial);
			IF MotorPosChange_PhaseShifter > Threshold_PhaseShifterMotorPos THEN
				Alarm_PhaseShifterMotorPos:=TRUE;
				ErrorID:=5001;
				ErrorDescription:='5001';
				Error:=TRUE;
				FB_PlcEmergencyStop();
				LED_Error:=TRUE;
			ELSE
				Alarm_PhaseShifterMotorPos:=FALSE;
			END_IF
			//电机速度监测
			IF Velo_PhaseShifterMotor > Threshold_PhaseShifterMotorVelo THEN
				Alarm_PhaseShifterMotorVelo:=TRUE;
				ErrorID:=5002;
				ErrorDescription:='5002';
				Error:=TRUE;
				FB_PlcEmergencyStop();
				LED_Error:=TRUE;
			ELSE
				Alarm_PhaseShifterMotorVelo:=FALSE;
			END_IF
    ELSE
        Alarm_PhaseShifterLin := FALSE;
	END_IF
END_IF
]]></ST>
      </Implementation>
    </Action>
