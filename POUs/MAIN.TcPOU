    <Action Name="A_InitAxisParameters" Id="{a92c2dd6-12b0-4a71-a4bc-d7afbc965cb4}" FolderPath="Axis\">
      <Implementation>
        <ST><![CDATA[	Pos_PhaseShifterLin:=DINT_TO_LREAL(LREAL_TO_DINT(PhaseShifterLin.NcToPlc.ActPos*100000))/100000;
	moveStatusPhaseShifterLin:=PhaseShifterLin.Status.Moving;
	Velo_PhaseShifterLin:=DINT_TO_LREAL(LREAL_TO_DINT(PhaseShifterLin.NcToPlc.ActVelo*100000))/100000;
	PosDiff_PhaseShifterLin_actual:=PhaseShifterLin.NcToPlc.PosDiff;
	PhaseShifterLin.ReadStatus();




]]></ST>
      </Implementation>
    </Action>
    <Action Name="A_InitRecipeParameters" Id="{6aa76980-7ae7-46e7-9688-f94b6fa6510d}" FolderPath="Recipe\">
      <Implementation>
        <ST><![CDATA[CASE state_loadParameters OF
	0:
		LoadParameter:=TRUE;
		Read_R_TRIG(CLK:=LoadParameter , Q=> );
		IF Read_R_TRIG.Q THEN
			state_loadParameters:=1;
		END_IF
	1:
		 ReturnDwordLoad:=RecipeManCommands.LoadAndWriteRecipe(RecipeDefinitionName:='Recipes' , RecipeName:='ParametersPS');
    	 LoadParameter:=FALSE;
		 IF ReturnDwordLoad<>0 THEN
		 	state_loadParameters:=-100;
		ELSE
			LoadRecipeParameters:=TRUE;
			LoadParameter:=FALSE;
			state_loadParameters:=88;
	 	END_IF
	88:
		loadParametersDone:=TRUE;
	-100:
		loadParametersDone:=FALSE;
		Error:=TRUE;
		ErrorId:=101;
		ErrorDescription:='LoadRecipeParameters Error';
		Plc_EmergencyStop:=FALSE;
END_CASE
//Save_R_TRIG(CLK:=SaveParameter , Q=> );
Read_R_TRIG(CLK:=LoadParameter , Q=> );
//IF Save_R_TRIG.Q THEN  //将plc数据以文件形式保存在工控机中
//	 RecipeManCommands.CreateRecipe(RecipeDefinitionName:='Recipes' , RecipeName:='ParametersPS2' );
//	 RecipeManCommands.ReadAndSaveRecipe(RecipeDefinitionName:='Recipes' , RecipeName:='ParametersPS2' );
//	 SaveParameter:=FALSE;
//END_IF
IF Read_R_TRIG.Q THEN  //将工控机中的文件数据读取到plc中
     ReturnDwordLoad:=RecipeManCommands.LoadAndWriteRecipe(RecipeDefinitionName:='Recipes' , RecipeName:='ParametersPS');
     LoadParameter:=FALSE;
	 IF ReturnDwordLoad<>0 THEN
		 Error:=TRUE;
		 ErrorId:=101;
		 ErrorDescription:='LoadRecipeParameters Error';
		 Plc_EmergencyStop:=FALSE;
	 END_IF
END_IF 
]]></ST>
      </Implementation>
    </Action>
    <Action Name="A_KillOpcIocShellExe" Id="{e0252576-ad9e-49ed-bcc5-7f64aacd5ffe}" FolderPath="TC\">
      <Implementation>
        <ST><![CDATA[R_TRIG_Exekill(CLK:=bExe_Kill_Prog, Q=> );
CASE Exe_kill_step OF
	0:
		IF R_TRIG_Exekill.Q THEN
			NT_StartProcess_Kill.START:=FALSE;
			NT_StartProcess_Kill.PATHSTR:='C:\HZB\OpcIocShell\IDControlIOC\killcmd.bat';
			//NT_StartProcess_Kill.DIRNAME:='C:\Program Files (x86)\HZB\OpcIocShell\bin\opcIocShell.exe';
			Exe_kill_step:=10;
		END_IF;
	10:
		NT_StartProcess_Kill.START:=TRUE;
		Exe_kill_step:=20;
	20:
		IF NT_StartProcess_Kill.ERR THEN
			Exe_kill_step:=0;
		ELSE
			Exe_kill_step:=30;
		END_IF;

	30:
		NT_StartProcess_Kill.START:=TRUE;
		Exe_kill_step:=40;
	40:
		IF NOT NT_StartProcess_Kill.BUSY THEN
			NT_StartProcess_Kill.START:=FALSE;
			Exe_Kill_Done:=TRUE;
			Exe_kill_step:=0;
		END_IF;
END_CASE;

NT_StartProcess_Kill(
	NETID:='' ,
	PATHSTR:= ,
	DIRNAME:= , 
	COMNDLINE:= , 
	START:= ,
	ERRID=> );
]]></ST>
      </Implementation>
    </Action>
    <Action Name="A_LoadOpcIocShellExe" Id="{e69e9ea4-205f-45b3-85e7-e1c1b3150dfb}" FolderPath="TC\">
      <Implementation>
        <ST><![CDATA[R_TRIG_Exeload(CLK:=bExe_load_Prog, Q=> );
CASE Exe_load_step OF
	0:
		IF R_TRIG_Exeload.Q THEN
			NT_StartProcess_Load.START:=FALSE;
			NT_StartProcess_Load.PATHSTR:='C:\HZB\OpcIocShell\IDControlIOC\startTwinCATOPCServer.bat';
			Exe_load_step:=10;
		END_IF;
	10:
		NT_StartProcess_Load.START:=TRUE;
		Exe_load_step:=20;
	20:
		IF NT_StartProcess_Load.ERR THEN
			Exe_load_step:=0;
		ELSE
			Exe_load_step:=30;
		END_IF;
	30:
		IF NOT NT_StartProcess_Load.BUSY THEN
			NT_StartProcess_Load.START:=FALSE;
			Exe_Load_Done:=TRUE;
			Exe_load_step:=0;
		END_IF;
END_CASE;

NT_StartProcess_Load(
	NETID:='' ,
	PATHSTR:= ,
	DIRNAME:= , 
	COMNDLINE:= , 
	START:= ,
	ERRID=> );
]]></ST>
      </Implementation>
    </Action>
    <Action Name="A_PosDiffDetectiion" Id="{e857c546-d367-4f19-9434-ed29c144eb6f}" FolderPath="Axis\">
      <Implementation>
        <ST><![CDATA[IF ABS(PosDiff_PhaseShifterLin_actual)>PosDiff_PhaseShifterLin THEN
	ErrorID:=201;
	Error:=TRUE;
	ErrorDescription:='ABS(PosDiff_PhaseShifterLin_actual)>PosDiff_PhaseShifterLin';
	FB_PlcEmergencyStop();
	LED_Error:=TRUE;
END_IF
]]></ST>
      </Implementation>
    </Action>
