    <Action Name="A_CheckLinearEncoder" Id="{04ef6a41-e2ba-498d-aa00-f823237bb5de}" FolderPath="Check\">
      <Implementation>
        <ST><![CDATA[//LEProtectionPosition
min_UserLEProtectionGapPosition:=min_UserGapPositionNew-min_ThresholdGapSoft2LEProtection;//回程误差有0.1mm
max_UserLEProtectionGapPosition:=max_UserGapPositionNew+max_ThresholdGapSoft2LEProtection;

//如果没有使用光栅、或者光栅参数没设置好，则不检查光栅位置
IF NOT USELinearEncoder OR NOT PositionBiasDone THEN
	RETURN;
END_IF
IF Pos_PhaseShifterLin <=0 THEN
	ErrorID:=1101;
	Error:=TRUE;
	ErrorDescription:='Pos_PhaseShifterLin <=0';
	FB_PlcEmergencyStop();
	LED_Error:=TRUE;
END_IF
//

IF Pos_PhaseShifterLin >max_UserLEProtectionGapPosition THEN
	ErrorID:=1105;
	Error:=TRUE;
	ErrorDescription:='Pos_PhaseShifterLin >max_UserLEProtectionGapPosition';
	FB_PlcEmergencyStop();
	LED_Error:=TRUE;
END_IF

//
IF Pos_PhaseShifterLin <min_UserLEProtectionGapPosition THEN
	ErrorID:=1109;
	Error:=TRUE;
	ErrorDescription:='Pos_PhaseShifterLin <min_UserLEProtectionGapPosition';
	FB_PlcEmergencyStop();
	LED_Error:=TRUE;
END_IF

]]></ST>
      </Implementation>
    </Action>
    <Action Name="A_CheckMaxVelocity" Id="{b95fb694-2a75-4423-bee6-d704fd7332fe}" FolderPath="Check\">
      <Implementation>
        <ST><![CDATA[IF NOT USELinearEncoder OR NOT PositionBiasDone OR NOT IDControlReady THEN
	RETURN;
END_IF
IF NOT st_Power THEN
	RETURN;
END_IF
//Monitoring Motor and LE Position/Velocity
MaxVelocityGlobalVelocity:=MAX(max_UserGapVelocityNew,max_UserTaperVelocityNew,UserMode.max_SS_StepVelocity,
			UserMode.Global_ChainProtection_MPS_MoveVelocity,UserMode.Global_ChainProtection_EPS_MoveVelocity);
IF 	ABS(Velo_PhaseShifterLin) > MaxVelocityGlobalVelocity*1.1/2 THEN
	ErrorID:=7001;
	Error:=TRUE;
	ErrorDescription:='ABS(Velo_PhaseShifterLin) > MaxVelocityGlobalVelocity*1.1/2';
	FB_PlcEmergencyStop();
	LED_Error:=TRUE;
END_IF
IF 	ABS(Velo_PhaseShifterMotor) > MaxVelocityGlobalVelocity*1.1/2 THEN
	ErrorID:=7002;
	Error:=TRUE;
	ErrorDescription:='ABS(Velo_PhaseShifterMotor) > MaxVelocityGlobalVelocity*1.1/2';
	FB_PlcEmergencyStop();
	LED_Error:=TRUE;
END_IF
]]></ST>
      </Implementation>
