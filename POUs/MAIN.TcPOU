    <Implementation>
      <ST><![CDATA[(*加载Ricipe参数*)
A_InitRecipeParameters();
(*Restart TwinCAT*)
IF Global_Variables.m_restartTC THEN
(*首先关闭opcIocShell.exe*)	
	bExe_Kill_Prog:=TRUE;
	A_KillOpcIocShellExe();
(*重启TC*)
	IF Exe_Kill_Done THEN
		A_RestartTC();
	END_IF
END_IF
(*******InitAxisParameters*******)
A_InitAxisParameters();
(*******CheckLimitSwitch*******)
A_CheckLimitSwitch();
(*Check Kill Switch*)
A_CheckKillSwitch();
(*******PosDiff¼à¿Ø*******)
A_PosDiffDetectiion();
(*******³õÊ¼»¯SafePlc*******)
IF NOT simulation THEN
A_SafePlcStartInit();
END_IF
(************SetAxisPosition*********************)
IF  NOT USELinearEncoder AND setAxisPositionEnable AND btn_setAxisPosition THEN
	//IF NOT SerAxisPositionDone THEN
		FB_Set_AxisPosition.ValueSet_PhaseShifterLin:=ValueSet_PhaseShifterLin;
		A_SetAxisPosition();
	//END_IF
END_IF
(*******³Mode Select******)
IF SafePLCRunOK OR Simulation THEN
	UserMode();
END_IF
(*******Led Show******)
fb_led_Show();
(*******Check Power status******)
FB_Power(Check:=TRUE);
st_Power:=FB_Power.PowerState;
(*********RS232*********************)
BackgroundEL();
(*fbEL6001(TxBuffer:=TxBufferEL, RxBuffer:=RxBufferEL);*)

(*设置position bias*)
IF loadParametersDone AND (DriveStatus=1 OR simulation) THEN 
	A_SetLinearEncoderPositionBias();
END_IF
(*检查硬急停状态，包括安全限位、柜门急停、手持急停*)
A_CheckHardwareEmergencyStopStatus();
(*获取驱动器状态信息*)
IF NOT simulation THEN
	A_GetDriverStatus();
END_IF
(*IDControlReady*)
IF ((DriveStatus<>0 AND SafePLCRunOK) OR simulation) AND NOT Error THEN
	IDControlReady:=TRUE;
ELSE
	IDControlReady:=FALSE;
END_IF

(*IOC Start*)
//IF IDControlReady THEN
	bExe_load_Prog:=TRUE;
	A_LoadOpcIocShellExe();
//END_IF

(*检查光栅读数是否在范围内*)
A_CheckLinearEncoder();

(*RestartDrivers*)
A_RestartDrivers();

(*A_LEHeartBeatMonitor*)
IF LEHeartBeatMonitorFunctionOn THEN
	A_LEHeartBeatMonitor();
END_IF
(************SetMotorAxisPosition*********************)
//IDControl 1s after
fb_ton5_2.IN:=IDControlReady;
fb_ton5_2.PT:=T#1S;
fb_ton5_2();
IF fb_ton5_2.Q THEN
	btn_SetMotorAxisPosition:=TRUE;
END_IF
IF  USELinearEncoder AND btn_SetMotorAxisPosition THEN
	IF NOT SetMotorAxisPositionDone THEN
		FB_Set_MotorAxisPosition.ValueSet_PhaseShifterMotor:=Pos_PhaseShifterLin;
		A_SetMotorAxisPosition();
	END_IF
END_IF
(*A_MonitorMotorAndLE*)
IF MonitorMotorAndLEFunctionOn AND SetMotorAxisPositionDone THEN
	A_MonitorMotorAndLE();
END_IF
(*A_CheckMaxVelocity*)
A_CheckMaxVelocity();]]></ST>
    </Implementation>
