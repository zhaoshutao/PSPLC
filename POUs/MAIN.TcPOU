    <Action Name="A_GetDriverStatus" Id="{e05cf975-7b3e-4430-bb2e-a308f4391f1b}" FolderPath="Driver\">
      <Implementation>
        <ST><![CDATA[IF bReloadDriveDiagMessageAndNumber THEN
	m_GetDriveStatusState:=0;
	bReloadDriveDiagMessageAndNumber:=FALSE;
END_IF
A_GetDriverStatusMC();
CASE m_GetDriveStatusState OF
	0:
		(*
		IF 	fb_SoEReadDiagMessage_LeftTop.bError
			OR fb_SoEReadDiagMessage_RightTop.bError
			OR fb_SoEReadDiagMessage_LeftBottom.bError
			OR fb_SoEReadDiagMessage_RightBottom.bError
			OR FB_SoEReadDiagNumber_LeftTop.bError
			OR FB_SoEReadDiagNumber_RightTop.bError
			OR FB_SoEReadDiagNumber_LeftBottom.bError
			OR FB_SoEReadDiagNumber_RightBottom.bError THEN
			m_GetDriveStatusState:=-100;
		ELSE
			m_GetDriveStatusState:=1;
		END_IF
		*)
		m_GetDriveStatusState:=1;
	1:
		bDriveDiagMessageAndNumber:=TRUE;
		m_GetDriveStatusState:=2;
	2:
		DriveDiagMessage_PhaseShifter:=fb_SoEReadDiagMessage_PhaseShifter.sDiagMessage.strData;
		//
		DriveDiagNumber_PhaseShifter:=FB_SoEReadDiagNumber_PhaseShifter.iDiagNumber;
		//
		IF 	NOT fb_SoEReadDiagMessage_PhaseShifter.bBusy
			AND NOT FB_SoEReadDiagNumber_PhaseShifter.bBusy THEN
			m_GetDriveStatusState:=88;
		END_IF
		IF 	fb_SoEReadDiagMessage_PhaseShifter.bError
			OR FB_SoEReadDiagNumber_PhaseShifter.bError THEN
			m_GetDriveStatusState:=-100;
		END_IF
		(*Drive Status*)
		//D012, Axis state machine: Control and power section ready
		IF DriveDiagNumber_PhaseShifter=16#D012 THEN
			DriveStatus:=1;
		//D013, Axis state machine: Axis in operation
		ELSIF DriveDiagNumber_PhaseShifter=16#D013 THEN
			DriveStatus:=2;
		ELSE
			DriveStatus:=0;
		END_IF
	88:
		bDriveDiagMessageAndNumber:=FALSE;
		A_GetDriverStatusMC();
		GetDriveStatusStateDone:=TRUE;
		GetDriveStatusStateError:=FALSE;
		m_GetDriveStatusState:=0;
	-100:
		bDriveDiagMessageAndNumber:=FALSE;
		A_GetDriverStatusMC();
		GetDriveStatusStateDone:=FALSE;
		GetDriveStatusStateError:=TRUE;
		Error:=TRUE;
		ErrorId:=1301;
		ErrorDescription:='GetDriveStatusStateError';
END_CASE

]]></ST>
      </Implementation>
    </Action>
