    <Action Name="A_InitAxisParameters" Id="{a92c2dd6-12b0-4a71-a4bc-d7afbc965cb4}" FolderPath="Axis\">
      <Implementation>
        <ST><![CDATA[//LE	
	Pos_PhaseShifterLin:=DINT_TO_LREAL(LREAL_TO_DINT(PhaseShifterLin.NcToPlc.ActPos*100000))/100000;
	moveStatusPhaseShifterLin:=PhaseShifterLin.Status.Moving;
	Velo_PhaseShifterLin:=DINT_TO_LREAL(LREAL_TO_DINT(PhaseShifterLin.NcToPlc.ActVelo*100000))/100000;
	PosDiff_PhaseShifterLin_actual:=DINT_TO_LREAL(LREAL_TO_DINT(PhaseShifterLin.NcToPlc.PosDiff*100000))/100000;
	Torque_PhaseShifterLin:=PhaseShifterLin.NcToPlc.ActTorque;	
	PhaseShifterLin.ReadStatus();

//Motor
	Pos_PhaseShifterMotor:=DINT_TO_LREAL(LREAL_TO_DINT(PhaseShifterMotor.NcToPlc.ActPos*100000))/100000;
	moveStatusPhaseShifterMotor:=PhaseShifterMotor.Status.Moving;
	Velo_PhaseShifterMotor:=DINT_TO_LREAL(LREAL_TO_DINT(PhaseShifterMotor.NcToPlc.ActVelo*100000))/100000;
	PosDiff_PhaseShifterMotor_actual:=DINT_TO_LREAL(LREAL_TO_DINT(PhaseShifterMotor.NcToPlc.PosDiff*100000))/100000;
	Torque_PhaseShifterMotor:=PhaseShifterMotor.NcToPlc.ActTorque;	
	PhaseShifterMotor.ReadStatus();

//
	//get Delta
	//
	//Delta Pos
	DeltaPos_PhaseShifter:=Pos_PhaseShifterLin - Pos_PhaseShifterMotor;
	//Delta Velocity	
	DeltaVelo_PhaseShifter:=Velo_PhaseShifterLin - Velo_PhaseShifterMotor;
	//Delta lagDistance
	DeltaPosDiff_PhaseShifter_actual:=PosDiff_PhaseShifterLin_actual - PosDiff_PhaseShifterMotor_actual;
	//Delta Torque
	DeltaTorque_PhaseShifter:=Torque_PhaseShifterLin - Torque_PhaseShifterMotor;
]]></ST>
      </Implementation>
    </Action>
    <Action Name="A_InitRecipeParameters" Id="{6aa76980-7ae7-46e7-9688-f94b6fa6510d}" FolderPath="Recipe\">
      <Implementation>
        <ST><![CDATA[CASE state_loadParameters OF
