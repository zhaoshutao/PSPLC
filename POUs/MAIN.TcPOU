    <Action Name="A_RestartDrivers" Id="{d1732994-27d5-4d0a-bd2e-fb077b65c9a9}" FolderPath="Driver\">
      <Implementation>
        <ST><![CDATA[Relay_EL2624[0]:=btn_RestartDriver1;
Relay_EL2624[1]:=btn_RestartDriver2;
Relay_EL2624[2]:=btn_RestartDriver3;
Relay_EL2624[3]:=btn_RestartDriver4;
Relay_EL2624[4]:=btn_RestartDriver5;
Relay_EL2624[5]:=btn_RestartDriver6;
Relay_EL2624[6]:=btn_RestartDriver7;
Relay_EL2624[7]:=btn_RestartDriver8;
///
//1.上升沿触发，btn_RestartDrivers上升沿后，btn_RestartDriver1~8全部高电平
//2、3s后，Driver1~8全部恢复低电平
//3、恢复全部状态到初始
///
RestartDriver_RTRIG(CLK:=btn_RestartDriversAll);
IF RestartDriver_RTRIG.Q THEN
	RestartDriver_Triggered:=TRUE;
	RestartDriver_Done:=FALSE;
END_IF
IF RestartDriver_Triggered THEN
	CASE RestartDriver_State OF
		0:
			btn_RestartDriver1:=TRUE;
			btn_RestartDriver2:=TRUE;
			btn_RestartDriver3:=TRUE;
			btn_RestartDriver4:=TRUE;
			btn_RestartDriver5:=TRUE;
			btn_RestartDriver6:=TRUE;
			btn_RestartDriver7:=TRUE;
			btn_RestartDriver8:=TRUE;
			//计时开始
			RestartDriver_TON(PT:=T#10S);
			RestartDriver_TON.IN:=TRUE;
			IF RestartDriver_TON.Q THEN
				RestartDriver_State:=1;
			END_IF
		1:
			btn_RestartDriver1:=FALSE;
			btn_RestartDriver2:=FALSE;
			btn_RestartDriver3:=FALSE;
			btn_RestartDriver4:=FALSE;
			btn_RestartDriver5:=FALSE;
			btn_RestartDriver6:=FALSE;
			btn_RestartDriver7:=FALSE;
			btn_RestartDriver8:=FALSE;
			RestartDriver_State:=88;
		88:
			RestartDriver_TON.IN:=FALSE;
			btn_RestartDriversAll:=FALSE;
			RestartDriver_State:=0;
			RestartDriver_Triggered:=FALSE;
			RestartDriver_Done:=TRUE;
	END_CASE
END_IF]]></ST>
      </Implementation>
    </Action>
