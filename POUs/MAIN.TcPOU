    <Action Name="A_CheckHardwareEmergencyStopStatus" Id="{c59a955b-0a8f-43a7-a0e7-3ab11b3d5ab6}" FolderPath="Check\">
      <Implementation>
        <ST><![CDATA[IF 	NOT KS_TopNear_Contact1 OR
