    <Action Name="A_GetDriverStatus" Id="{e05cf975-7b3e-4430-bb2e-a308f4391f1b}" FolderPath="Driver\">
      <Implementation>
        <ST><![CDATA[//
//DriveMessage/Drive Number/Gear number/feedback2 direction
//
IF bReloadDriveDiagMessageAndNumber THEN
	m_GetDriveStatusState:=0;
	bReloadDriveDiagMessageAndNumber:=FALSE;
END_IF
GetDriveStatusTON(PT:=T#1S);
GetDriveStatusTON2(PT:=T#0.1S);
A_GetDriverStatusMC();
CASE m_GetDriveStatusState OF
	0:
		GetDriveStatusTON.IN:=TRUE;
		m_GetDriveStatusState:=1;
	1:
		IF GetDriveStatusTON.Q THEN
			bDriveDiagMessageAndNumber:=TRUE;
			m_GetDriveStatusState:=2;
		END_IF
	2:
		GetDriveStatusTON.IN:=FALSE;
		DriveDiagMessage_PhaseShifter:=fb_SoEReadDiagMessage_PhaseShifter.sDiagMessage.strData;
		//
		DriveDiagNumber_PhaseShifter:=FB_SoEReadDiagNumber_PhaseShifter.iDiagNumber;
		//
		GearNumeratorGet_PhaseShifter:=st_p182_PhaseShifter.Feedback_2_geat_numerator;
		//
		GearDenominatorGet_PhaseShifter:=st_p183_PhaseShifter.Feedback_2_geat_denominator;
		//
		Feedback2directionGet_PhaseShifter:=st_p180_PhaseShifter.Feedback_direction;
		//
		IF 	NOT fb_SoEReadDiagMessage_PhaseShifter.bBusy
			AND NOT FB_SoEReadDiagNumber_PhaseShifter.bBusy
			AND NOT FB_SoERead_GearNumerator_PhaseShifter.bBusy 
			AND NOT FB_SoERead_GearDenominator_PhaseShifter.bBusy 
			AND NOT FB_SoERead_Feedback2direction_PhaseShifter.bBusy  THEN
				GetDriveStatusTON2.IN:=TRUE;
				IF GetDriveStatusTON2.Q THEN
					m_GetDriveStatusState:=88;
				END_IF
		END_IF
		IF 	fb_SoEReadDiagMessage_PhaseShifter.bError
			OR FB_SoEReadDiagNumber_PhaseShifter.bError
			OR FB_SoERead_GearNumerator_PhaseShifter.bError 
			OR FB_SoERead_GearDenominator_PhaseShifter.bError 
			OR FB_SoERead_Feedback2direction_PhaseShifter.bError 
		THEN
			m_GetDriveStatusState:=-100;
		END_IF
		
	88:
		bDriveDiagMessageAndNumber:=FALSE;
		A_GetDriverStatusMC();
		GetDriveStatusStateDone:=TRUE;
		GetDriveStatusStateError:=FALSE;
		//
		(*Drive Status*)
		//D012, Axis state machine: Control and power section ready,53266
		IF DriveDiagNumber_PhaseShifter=16#D012  THEN
			DriveStatus:=1;
		//D013, Axis state machine: Axis in operation,53267
		ELSIF DriveDiagNumber_PhaseShifter=16#D013  THEN
			DriveStatus:=2;
		//D014，Axis state machine: Axis halt.53266和53267中间会经历这么一个状态，算正常状态
		ELSIF DriveDiagNumber_PhaseShifter=16#D014  THEN
			DriveStatus:=3;
		ELSE
			DriveStatus:=0;
		END_IF
		//
		//
		//	//check statusGet and status configured
		IF 	GearNumeratorGet_PhaseShifter<>GearNumeratorConfigured_PhaseShifter 
			OR GearDenominatorGet_PhaseShifter<>GearDenominatorConfigured_PhaseShifter   THEN
				bDriveDiagMessageAndNumber:=FALSE;
				A_GetDriverStatusMC();
				GetDriveStatusStateDone:=FALSE;
				GetDriveStatusStateError:=TRUE;
				Error:=TRUE;
				ErrorId:=1302;
				ErrorDescription:='gearParameters Error';
				Plc_EmergencyStop:=FALSE;
		END_IF
		IF USELinearEncoder THEN
			IF  Feedback2directionGet_PhaseShifter<>Feedback2directionConfigured_PhaseShifter THEN
					bDriveDiagMessageAndNumber:=FALSE;
					A_GetDriverStatusMC();
					GetDriveStatusStateDone:=FALSE;
					GetDriveStatusStateError:=TRUE;
					Error:=TRUE;
					ErrorId:=1303;
					ErrorDescription:='fb2direction Error';
					Plc_EmergencyStop:=FALSE;
			END_IF
		END_IF
		m_GetDriveStatusState:=0;
	-100:
		bDriveDiagMessageAndNumber:=FALSE;
		A_GetDriverStatusMC();
		GetDriveStatusStateDone:=FALSE;
		GetDriveStatusStateError:=TRUE;
		Error:=TRUE;
		ErrorId:=1301;
		ErrorDescription:='GetDriveStatusStateError';
END_CASE
]]></ST>
