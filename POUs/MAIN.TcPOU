    <Action Name="A_SafePlcStartInit" Id="{4d1a8480-9923-4117-8ca7-ea613a82dfad}" FolderPath="Safe\">
      <Implementation>
        <ST><![CDATA[fb_ton0(PT:=t#1s);
fb_ton1(PT:=t#0.1s);
fb_ton2(PT:=t#0.1s);
fb_ton3(PT:=t#0.1s);

CASE SafeStatus OF
	0:
		IF COM_ERROR OR NOT Estop_Out THEN
			Err_Ack:=TRUE;
			fb_ton0.IN:=TRUE;
			SafeStatus:=1;
		ELSE
			SafePLCRunOK:=TRUE;
			SafeStatus:=0;
		END_IF
	1:
		IF fb_ton0.Q THEN
			Err_Ack:=FALSE;
			Safe_run:=TRUE;
			fb_ton1.IN:=TRUE;
			SafeStatus:=2;
		END_IF

	2:
		IF fb_ton1.Q THEN
			IF Estop_RunAndRestart=FALSE THEN
				Estop_RunAndRestart:=TRUE;
				fb_ton2.IN:=TRUE;
				SafeStatus:=3;
			END_IF
		END_IF
	3:
		IF fb_ton2.Q THEN
			IF Estop_RunAndRestart THEN
				Estop_RunAndRestart:=FALSE;
				fb_ton3.IN:=TRUE;
				SafeStatus:=4;
			END_IF
		END_IF
	4:
		IF fb_ton3.Q THEN
			IF Estop_Out THEN
				SafePLCRunOK:=TRUE;
				SafeStatus:=0;
			ELSE
				SafePLCRunOK:=FALSE;
				//Error:=TRUE;
				//ErrorId:=301;
				//ErrorDescription:='SafePLCRunOK fail';
			END_IF
		fb_ton0.IN:=FALSE;
		fb_ton1.IN:=FALSE;
		fb_ton2.IN:=FALSE;
		fb_ton3.IN:=FALSE;
		END_IF


END_CASE]]></ST>
      </Implementation>
    </Action>
    <Action Name="A_SetAxisPosition" Id="{eb32aedd-8f01-44b1-96a9-73f233ea8548}" FolderPath="Axis\">
      <Implementation>
        <ST><![CDATA[fb_ton4(PT:=t#0.5s);
CASE SetAxisPosState OF
	0:	(*FB_Set_AxisPosition*)
		FB_Set_AxisPosition(Execute:=TRUE);
		fb_ton4.IN:=TRUE;
		SetAxisPosState:=1;
	1:	(*wait*)
		FB_Set_AxisPosition();
		IF NOT FB_Set_AxisPosition.Done THEN		(*FB_Set_AxisPositionÃ»ÓÐÖ´ÐÐÍê£¬wait£¬³¬Ê±FB Error*)
			IF NOT fb_ton4.Q THEN
				SetAxisPosState:=1;
			ELSE
				SetAxisPosState:=99;
			END_IF
		ELSE
			SetAxisPosState:=88;
		END_IF
	88:
		SetAxisPosState:=0;
		SetAxisPositionDone:=TRUE;
		Error:=FALSE;
		ErrorID:=0;
		FB_Set_AxisPosition.Execute:=FALSE;
		fb_ton4.IN:=FALSE;
	99:
		SetAxisPosState:=99;
		SetAxisPositionDone:=FALSE;
		Error:=TRUE;
		FB_Set_AxisPosition.Execute:=FALSE;
		fb_ton4.IN:=FALSE;
		LED_Error:=TRUE;
		ErrorID:=401;
		ErrorDescription:='SetAxisPosition fail';
END_CASE
]]></ST>
      </Implementation>
    </Action>
    <Action Name="A_SetLinearEncoderPositionBias" Id="{6f3d3f8e-255d-4e09-853f-5fdf91122229}" FolderPath="LinearEncoder\">
      <Implementation>
        <ST><![CDATA[//设置bias开始
A_SetLinearEncoderPositionBiasMC();
PositionBiasTON(PT:=T#0.1S);
CASE state_setLEPosBias OF
	0:
		PositionBiasTON.IN:=TRUE;
		IF MC_SetLEPosBias_PhaseShifter.Error THEN
			state_setLEPosBias:=-100;
		ELSE 
			state_setLEPosBias:=1;
		END_IF
	1:
		IF NOT MC_SetLEPosBias_PhaseShifter.Done  THEN
			MC_SetLEPosBias_PhaseShifter.Execute:=TRUE;
			state_setLEPosBias:=2;
		ELSE
			state_setLEPosBias:=-100;
		END_IF
	2:
		IF NOT MC_SetLEPosBias_PhaseShifter.Done THEN
			IF NOT PositionBiasTON.Q THEN
				state_setLEPosBias:=2;
			ELSE
				state_setLEPosBias:=-100;
			END_IF
		END_IF
		IF  MC_SetLEPosBias_PhaseShifter.Done THEN
			state_setLEPosBias:=88;
		END_IF	
		IF MC_SetLEPosBias_PhaseShifter.Error THEN
			state_setLEPosBias:=-100;
		END_IF 
		
	88:
		PositionBiasTON.IN:=FALSE;
		PositionBiasDone:=TRUE;
	-100:
		PositionBiasTON.IN:=FALSE;
		PositionBiasDone:=FALSE;
		Error:=TRUE;
		ErrorId:=901;
		ErrorDescription:='Set LinearEncoderPositionBias Error';
		Plc_EmergencyStop:=FALSE;
END_CASE]]></ST>
      </Implementation>
    </Action>
